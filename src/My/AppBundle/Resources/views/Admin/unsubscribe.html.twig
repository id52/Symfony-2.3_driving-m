{% extends 'AppBundle::admin.html.twig' %}
{% trans_default_domain 'profile' %}

{% block content %}
    <div class="mainContent">
        <section class="unsubscribe">
            <div class="title"><h1>{{ 'unsubscribe_done_title'|trans }}</h1></div>
        </section>
        <section class="unsubscribe_body">
            <div class="title">{{ 'unsubscribe'|trans({ '%userName%': userName }) }}</div>
        </section>
    </div>
{% endblock content %}

{% block js %}
    <script>
        $(function() {
            setTimeout(function () {
                location.assign('{{ url }}');
            }, 5000)
        });
    </script>
{% endblock js %}

